<%- include("header") -%>
        <div class="box" id="heading">
            <h1><%= listTittle %></h1>
        </div>

        <div class="box">
                
            <% for (var i = 0; i < newListItems.length; i++){ %>
                <div class="item">
                    <input type="checkbox">
                    <p><%= newListItems[i] %></p>
                </div>

            <% } %>

            <form class="item" action="/" method="POST">
                <input type="text" name="newItem" placeholder="Add Item" autocomplete="off" required>
                <button  type="submit" name="list" value=<%= listTittle %>>+</button>
            </form>

        </div>
        <%- include("footer") -%>
  